<RuleSet
	xmlns="uri://org.msscf/msscf/2.0.13/cfgenkb-2.13-ruleset"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xsi:schemaLocation="uri://org.msscf/msscf/2.0.13/cfgenkb-2.13-ruleset file:/opt/msscf/2.0.13/xsd/cfgenkb-2.13-ruleset.xsd"
	ToolSet="db2luw"
	Name="schema/dbcreate/db2luw/crrel_table_idg64.sql"
	Revision="2.13"
	Descr="IBM DB/2 LUW create table Id64 generator sql script">

	<GenFile ScopeDef="Table" GenDef="Id64Gen" Name="dbadminfile_CreateTableIdg64Relations"
			GenerateOnce="false"
			ModuleName=""
		SourceBundle="dbcreate"
		BasePackageName="$lower SchemaName$"
		SubPackageName="db2luw"
		ExpansionClassName="crrel_$lower DbTableName$_idg64"
		ExpansionKeyName="crrel_$lower DbTableName$_idg64"
		ExpansionFileName="$dbadminfilename_CreateTableIdg64Relations$">--
--	@(#) $GenPackageDir$/$GenFileName$
$MssDoubleDashScriptLicense$
ALTER TABLE $lower DbSchemaName$.$DbTableName$_idg64 ADD CONSTRAINT $DbTableName$_idg64_prnt$ContSqlStmt$	FOREIGN KEY ($reference Dispenser reference PrimaryKeyIndex iterate Columns( each clause_PKeyColComma last clause_PKeyCol )$$ContSqlStmt$	) REFERENCES $lower DbSchemaName$.$reference Dispenser DbTableName$ ($reference Dispenser reference PrimaryKeyIndex iterate Columns( each clause_PKeyColComma last clause_PKeyCol )$ )
	ON DELETE CASCADE$RunSqlStmt$
</GenFile>

</RuleSet>
