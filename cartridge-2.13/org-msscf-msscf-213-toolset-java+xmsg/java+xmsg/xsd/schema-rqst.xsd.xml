<RuleSet
	xmlns="uri://org.msscf/msscf/2.0.13/cfgenkb-2.13-ruleset"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xsi:schemaLocation="uri://org.msscf/msscf/2.0.13/cfgenkb-2.13-ruleset file:/opt/msscf/2.0.13/xsd/cfgenkb-2.13-ruleset.xsd"
	ToolSet="java+xmsg"
	Name="xsd/project-schema-rqst.xsd"
	Revision="2.13"
	Descr="Java 11 XML Request Message Schema Definition">

	<GenFile GenDef="SchemaDef" Name="fileSchemaRqstXsd"
			GenerateOnce="false"
			ModuleName=""
		SourceBundle="java"
		BasePackageName="$reference ManufacturingSchema lower PackageName$xmsgrqst"
		SubPackageName="xsd"
		ExpansionClassName="$SchemaName$RqstXsd"
		ExpansionKeyName="$SchemaName$RqstXsd"
		ExpansionFileName="$lower SchemaName$-rqst.xsd"
>$XsdXmlPreamble$&lt;xsd:schema xmlns="$SchemaXMsgRqstNamespace$" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" targetNamespace="$SchemaXMsgRqstNamespace$" version="$reference MinorVersion reference ParentMajVer Name$.$reference MinorVersion Name$"&gt;
$SchemaRqstAnnotation$$iterate Types( first CommentDeclRqstAtomType each DeclRqstAtomType empty empty )$$iterate SchemaTables( first CommentDeclRqstTableColTypes each DeclRqstTableColTypes empty empty )$$DeclRqstFileImportAttributeTypes$$DeclRqstLogInAttributeTypes$$DeclRqstLogOutAttributeTypes$$iterate SchemaTables( first CommentDeclRqstIndexType each DeclRqstIndexType empty empty )$$CommentRqstTableObjTypes$$iterate SchemaTables( each DeclRqstTableRootObjTypes empty empty )$$JavaXMsgRqstSchemaXsdSpec$$ReferencedJavaXMsgRqstSchemaXsdSpec$$DeclRqstFileImportType$$DeclRqstLogInType$$DeclRqstLogOutType$$DeclRqstRootDocument$
&lt;/xsd:schema&gt;
</GenFile>

<GenRule GenDef="SchemaDef" Name="DeclRqstFileImportAttributeTypes">
&lt;xsd:simpleType name="FileImportFileName"&gt;
	&lt;xsd:restriction base="xsd:string"/&gt;
&lt;/xsd:simpleType&gt;

&lt;xsd:simpleType name="FileImportFileContent"&gt;
	&lt;xsd:restriction base="xsd:string"&gt;
		&lt;xsd:whiteSpace value="preserve"/&gt;
	&lt;/xsd:restriction&gt;
&lt;/xsd:simpleType&gt;
</GenRule>

<GenRule GenDef="SchemaDef" Name="DeclRqstFileImportType">
&lt;xsd:complexType name="RqstFileImportType" mixed="true"&gt;
	&lt;xsd:sequence&gt;
	&lt;/xsd:sequence&gt;
	&lt;xsd:attribute name="FileName" type="FileImportFileName" use="required" /&gt;
	&lt;xsd:attribute name="FileContent" type="FileImportFileContent" use="required" /&gt;
&lt;/xsd:complexType&gt;
</GenRule>

<GenRule GenDef="SchemaDef" Name="DeclRqstLogInAttributeTypes">
&lt;xsd:simpleType name="LogInDevEncPWHashType"&gt;
	&lt;xsd:restriction base="xsd:base64Binary"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>

<GenRule GenDef="SchemaDef" Name="DeclRqstLogInType">
&lt;xsd:complexType name="RqstLogInType" mixed="true"&gt;
	&lt;xsd:sequence&gt;
	&lt;/xsd:sequence&gt;
	&lt;xsd:attribute name="LoginId" type="SecUserLoginIdType" use="required" /&gt;
	&lt;xsd:attribute name="DeviceName" type="DeviceNameType" use="required" /&gt;
	&lt;xsd:attribute name="DevEncPWHash" type="LogInDevEncPWHashType" use="required" /&gt;
	&lt;xsd:attribute name="ClusterName" type="ClusterFullDomNameType" use="required" /&gt;
	&lt;xsd:attribute name="TenantName" type="TenantTenantNameType" use="required" /&gt;
&lt;/xsd:complexType&gt;
</GenRule>

<GenRule GenDef="SchemaDef" Name="DeclRqstLogOutAttributeTypes">
&lt;xsd:simpleType name="LogOutSecSessionIdType"&gt;
	&lt;xsd:restriction base="SecSessionIdGenType"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>

<GenRule GenDef="SchemaDef" Name="DeclRqstLogOutType">
&lt;xsd:complexType name="RqstLogOutType" mixed="true"&gt;
	&lt;xsd:sequence&gt;
	&lt;/xsd:sequence&gt;
	&lt;xsd:attribute name="SecSessionId" type="LogOutSecSessionIdType" use="required" /&gt;
&lt;/xsd:complexType&gt;
</GenRule>

	<GenRule GenDef="SchemaDef" Name="ReferencedJavaXMsgRqstSchemaXsdSpec"
		>$iterate SchemaRefs( each ApplyJavaXMsgRqstSchemaXsdSpec empty empty )$</GenRule>

	<GenRule GenDef="SchemaRef" Name="ApplyJavaXMsgRqstSchemaXsdSpec"
		>$reference RefSchema JavaXMsgRqstSchemaXsdSpec$</GenRule>

	<GenRule GenDef="Object" Name="XsdXmlPreamble"
>&lt;?xml version="1.0" encoding="UTF-8" standalone="no"?>
</GenRule>

	<GenRule GenDef="MajorVersion" Name="DashPackedVersion">-$lower PackedVersion$</GenRule>

	<GenRule GenDef="MinorVersion" Name="DashPackedVersion">-$lower PackedVersion$</GenRule>

	<GenRule GenDef="SchemaDef" Name="DashPackedVersion">-$lower PackedVersion$</GenRule>

	<GenRule GenDef="SchemaDef" Name="DeclRqstRootDocument">
&lt;!-- ************************************************************************
 *
 *	Schema Root Document
 *
 **************************************************************************--&gt;

&lt;xsd:complexType name="$SchemaName$RqstType" mixed="true"&gt;
	&lt;xsd:sequence&gt;
		&lt;xsd:choice minOccurs="0" maxOccurs="unbounded"&gt;$DeclRqstFileImportElement$$DeclRqstLogInElement$$DeclRqstLogOutElement$$iterate SchemaTables( each DeclRqstRootObjElements empty empty )$$JavaXMsgRqstSchemaXsdElementList$
		&lt;/xsd:choice&gt;
	&lt;/xsd:sequence&gt;
&lt;/xsd:complexType&gt;

&lt;xsd:element name="$SchemaName$Rqst" type="$SchemaName$RqstType" /&gt;
</GenRule>

	<GenRule GenDef="SchemaDef" Name="DeclRqstFileImportElement">
			&lt;xsd:element name="RqstFileImport" type="RqstFileImportType" /&gt;</GenRule>

	<GenRule GenDef="SchemaDef" Name="DeclRqstLogInElement">
			&lt;xsd:element name="RqstLogIn" type="RqstLogInType" /&gt;</GenRule>

	<GenRule GenDef="SchemaDef" Name="DeclRqstLogOutElement">
			&lt;xsd:element name="RqstLogOut" type="RqstLogOutType" /&gt;</GenRule>

	<GenRule GenDef="SchemaDef" Name="SchemaXMsgRqstNamespace"
		>uri://$lower reference MinorVersion reference ParentMajVer reference ParentSPrj reference ParentTPrj reference ParentSDom reference ParentTld Name$.$lower reference MinorVersion reference ParentMajVer reference ParentSPrj reference ParentTPrj reference ParentSDom Name$/$lower reference MinorVersion reference ParentMajVer reference ParentSPrj reference ParentTPrj Name$/$reference MinorVersion reference ParentMajVer Name$.0.$reference MinorVersion Name$/$lower SchemaName$rqst</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstRootObjElements"
		>$DeclRqstCreateTableElement$$DeclRqstReadTableElement$$DeclRqstLockTableElement$$DeclRqstUpdateTableElement$$DeclRqstDeleteElement$$switch HasChain yes DeclRqstMoveUpElement default empty$$switch HasChain yes DeclRqstMoveDownElement default empty$$DeclRqstReadAllElement$$switch PageData yes DeclRqstPageAllElement default empty$$DeclRqstRdByIndexElements$$switch PageData yes DeclRqstPageByIndexElements default empty$$DeclRqstDelByIndexElements$$iterate ServerMethods( each DeclRqstServerMethodElement empty empty )$</GenRule>

	<GenRule GenDef="ServerMethod" Name="DeclRqstServerMethodElement">
			&lt;xsd:element name="Rqst$TableName$$Name$" type="$TableName$Rqst$Name$Type" /&gt;</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstDelByIndexElements"
		>$switch HasSuperClassRelation yes DeclRqstDelByInhIndexElements default DeclRqstDelByBaseIndexElements$</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstDelByInhIndexElements"
		>$reference SuperClassRelation reference ToTable DeclRqstDelByIndexElements$$iterate Index( each DeclRqstDelByNonPrimaryIndexElement empty empty )$</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstDelByBaseIndexElements"
		>$iterate Index( each DeclRqstDelByNonPrimaryIndexElement empty empty )$</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstRdByIndexElements"
		>$switch HasSuperClassRelation yes DeclRqstRdByInhIndexElements default DeclRqstRdByBaseIndexElements$</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstRdByInhIndexElements"
		>$reference SuperClassRelation reference ToTable DeclRqstRdByIndexElements$$iterate Index( each DeclRqstReadTableByNonPrimaryIndexElement empty empty )$</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstRdByBaseIndexElements"
		>$iterate Index( each DeclRqstReadTableByNonPrimaryIndexElement empty empty )$</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstReadTableElement">
			&lt;xsd:element name="Rqst$poptop Table TableName$Read" type="$poptop Table TableName$RqstReadType" /&gt;</GenRule>

	<GenRule GenDef="Index" Name="DeclRqstReadTableByNonPrimaryIndexElement"
		>$switch IsPrimaryIndex yes empty default DeclRqstReadTableByIndexElement$</GenRule>

	<GenRule GenDef="Index" Name="DeclRqstReadTableByIndexElement">
			&lt;xsd:element name="Rqst$poptop Table TableName$RdBy$Suffix$" type="$poptop Table TableName$RqstRdBy$Suffix$Type" /&gt;</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstPageByIndexElements"
		>$switch HasSuperClassRelation yes DeclRqstPageByInhIndexElements default DeclRqstPageByBaseIndexElements$</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstPageByInhIndexElements"
		>$reference SuperClassRelation reference ToTable DeclRqstPageByIndexElements$$iterate Index( each DeclRqstPageTableByNonPrimaryIndexElement empty empty )$</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstPageByBaseIndexElements"
		>$iterate Index( each DeclRqstPageTableByNonPrimaryIndexElement empty empty )$</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstPageTableElement">
			&lt;xsd:element name="Rqst$poptop Table TableName$Page" type="$poptop Table TableName$RqstPageType" /&gt;</GenRule>

	<GenRule GenDef="Index" Name="DeclRqstPageTableByNonPrimaryIndexElement"
		>$switch IsPrimaryIndex yes empty default ifduplicate_DeclRqstPageTableByIndexElement$</GenRule>

	<GenRule GenDef="Index" Name="ifduplicate_DeclRqstPageTableByIndexElement"
		>$switch IsUnique yes empty default DeclRqstPageTableByIndexElement$</GenRule>

	<GenRule GenDef="Index" Name="DeclRqstPageTableByIndexElement">
			&lt;xsd:element name="Rqst$poptop Table TableName$PageBy$Suffix$" type="$poptop Table TableName$RqstPageBy$Suffix$Type" /&gt;</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstReadAllElement">
			&lt;xsd:element name="Rqst$poptop Table TableName$ReadAll" type="$poptop Table TableName$RqstReadAllType" /&gt;</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstPageAllElement">
			&lt;xsd:element name="Rqst$poptop Table TableName$PageAll" type="$poptop Table TableName$RqstPageAllType" /&gt;</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstLockTableElement">
			&lt;xsd:element name="Rqst$poptop Table TableName$Lock" type="$poptop Table TableName$RqstLockType" /&gt;</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstCreateTableElement">
			&lt;xsd:element name="Rqst$poptop Table TableName$Create" type="$poptop Table TableName$RqstCreateType" /&gt;</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstUpdateTableElement">
			&lt;xsd:element name="Rqst$poptop Table TableName$Update" type="$poptop Table TableName$RqstUpdateType" /&gt;</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstDeleteElement">
			&lt;xsd:element name="Rqst$poptop Table TableName$Delete" type="$poptop Table TableName$RqstDeleteType" /&gt;</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstMoveUpElement">
			&lt;xsd:element name="Rqst$poptop Table TableName$MoveUp" type="$poptop Table TableName$RqstMoveUpType" /&gt;</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstMoveDownElement">
			&lt;xsd:element name="Rqst$poptop Table TableName$MoveDown" type="$poptop Table TableName$RqstMoveDownType" /&gt;</GenRule>

	<GenRule GenDef="Index" Name="DeclRqstDelByNonPrimaryIndexElement"
		>$switch IsPrimaryIndex yes empty default DeclRqstDelByIndexElement$</GenRule>

	<GenRule GenDef="Index" Name="DeclRqstDelByIndexElement">
			&lt;xsd:element name="Rqst$poptop Table TableName$DelBy$Suffix$" type="$poptop Table TableName$RqstDelBy$Suffix$Type" /&gt;</GenRule>

	<GenRule GenDef="SchemaDef" Name="SchemaRqstAnnotation">
&lt;xsd:annotation&gt;
	&lt;xsd:documentation xml:lang="en"&gt;
		$EffectiveProjectName$ request messages for schema $SchemaName$
	&lt;/xsd:documentation&gt;
&lt;/xsd:annotation&gt;
</GenRule>

	<GenRule GenDef="Atom" Name="CommentDeclRqstAtomType">
&lt;!-- ************************************************************************
 *
 *	Schema Atomic Types
 *
 **************************************************************************--&gt;
$DeclRqstAtomType$</GenRule>

	<GenRule GenDef="Table" Name="CommentDeclRqstTableColTypes">
&lt;!-- ************************************************************************
 *
 *	Table Column Types
 *
 **************************************************************************--&gt;
$DeclRqstTableColTypes$</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstTableColTypes"
		>$iterate Columns( each DeclRqstTableColType empty empty )$$DeclRqstTableColRevisionType$</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstTableColRevisionType">
&lt;xsd:simpleType name="$Name$RevisionType"&gt;
	&lt;xsd:restriction base="xsd:integer"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>

	<GenRule GenDef="BlobDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>

	<GenRule GenDef="BoolDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="EnumDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="Int16Def" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="Int32Def" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="Int64Def" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="UInt16Def" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="UInt32Def" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="UInt64Def" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="FloatDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="DoubleDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="NumberDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="StringDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"&gt;
		&lt;xsd:maxLength value="$MaxLen$"/&gt;
		&lt;xsd:whiteSpace value="preserve"/&gt;
	&lt;/xsd:restriction&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="TokenDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$" /&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="NmTokenDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$" /&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="NmTokensDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$" /&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="TextDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"&gt;
		&lt;xsd:maxLength value="$MaxLen$"/&gt;
		&lt;xsd:whiteSpace value="preserve"/&gt;
	&lt;/xsd:restriction&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="DateDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="TimeDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="TimestampDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="TZDateDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="TZTimeDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="TZTimestampDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="UuidDef" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="TableCol" Name="DeclRqstTableColType">
&lt;xsd:simpleType name="$TableName$$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"&gt;$switch BaseModelAtomClass StringDef DeclRqstTableColMaxLenPreserveWhitespace TextDef DeclRqstTableColMaxLenPreserveWhitespace default empty$
	&lt;/xsd:restriction&gt;
&lt;/xsd:simpleType&gt;
</GenRule>

	<GenRule GenDef="TableCol" Name="DeclRqstTableColMaxLenPreserveWhitespace">
		&lt;xsd:maxLength value="$MaxLen$"/&gt;
		&lt;xsd:whiteSpace value="preserve"/&gt;</GenRule>

	<GenRule GenDef="Table" Name="CommentDeclRqstIndexType">
&lt;!-- ************************************************************************
 *
 *	Schema Request Primary Index Types
 *
 **************************************************************************--&gt;
$DeclRqstIndexType$</GenRule>

	<GenRule GenDef="Table" Name="DeclRqstIndexType"
		>$reference PrimaryKeyIndex DeclIndexType$</GenRule>

	<GenRule GenDef="SchemaDef" Name="CommentRqstTableObjTypes">
&lt;!-- ************************************************************************
 *
 *	Schema Table Request Object Types
 *
 **************************************************************************--&gt;
</GenRule>

	<GenRule GenDef="BoolDef" Name="DeclRqstAtomType">
&lt;xsd:simpleType name="$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>

	<GenRule GenDef="EnumDef" Name="DeclRqstAtomType">
&lt;xsd:simpleType name="$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"&gt;
		&lt;xsd:minLength value="1"/&gt;
		&lt;xsd:maxLength value="127"/&gt;
		&lt;xsd:whiteSpace value="collapse"/&gt;$iterate Tag( each declEnumTypeEnumerationTag )$
	&lt;/xsd:restriction&gt;
&lt;/xsd:simpleType&gt;
</GenRule>

	<GenRule GenDef="EnumTag" Name="declEnumTypeEnumerationTag">
		&lt;xsd:enumeration value="$Name$"/&gt;</GenRule>

	<GenRule GenDef="Int16Def" Name="DeclRqstAtomType">
&lt;xsd:simpleType name="$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="Int32Def" Name="DeclRqstAtomType">
&lt;xsd:simpleType name="$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="Int64Def" Name="DeclRqstAtomType">
&lt;xsd:simpleType name="$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
	<GenRule GenDef="UInt16Def" Name="DeclRqstAtomType">
&lt;xsd:simpleType name="$Name$Type"&gt;
	&lt;xsd:restriction base="$XsdBaseType$"/&gt;
&lt;/xsd:simpleType&gt;
</GenRule>
