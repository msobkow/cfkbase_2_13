<RuleSet
	xmlns="uri://org.msscf/msscf/2.0.13/cfgenkb-2.13-ruleset"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xsi:schemaLocation="uri://org.msscf/msscf/2.0.13/cfgenkb-2.13-ruleset file:/opt/msscf/2.0.13/xsd/cfgenkb-2.13-ruleset.xsd"
	ToolSet="java+xmsg"
	Name="Top"
	Revision="2.13"
	Descr="Top level rules for building files">

	<GenRule GenDef="MinorVersion" Name="top"
		>$iterate SchemaDef( each schemaFiles )$</GenRule>

	<GenRule GenDef="Relation" Name="ToTableHasClassCode"
		>$reference ToTable HasClassCode$</GenRule>

	<GenRule GenDef="Relation" Name="ToTableHasLookupIndex"
		>$reference ToTable HasLookupIndex$</GenRule>

	<GenRule GenDef="RelationCol" Name="ToTableHasClassCode"
		>$reference Relation reference ToTable HasClassCode$</GenRule>

	<GenRule GenDef="RelationCol" Name="ToTableHasLookupIndex"
		>$reference Relation reference ToTable HasLookupIndex$</GenRule>

	<GenRule GenDef="SchemaDef" Name="schemaFiles"
		>$validatePageData$$fileSchemaXMsgGitIgnore$$fileSchemaXMsgClntGitIgnore$$fileSchemaXMsgRqstGitIgnore$$fileSchemaXMsgRspnGitIgnore$$fileSchemaRqstXsd$$fileSchemaRspnXsd$$iterate SchemaRefs( each schemaImportedXsdFiles empty empty )$$fileSchemaXMsgSchemaMessageFormatterJava$$fileSchemaXMsgClntSchemaJava$$fileSchemaXMsgClntHttpSchemaJava$$fileSchemaXMsgRqstParserJava$$fileSchemaXMsgRqstFileImportJava$$fileSchemaXMsgRqstLogInJava$$fileSchemaXMsgRqstLogOutJava$$fileSchemaXMsgRspnParserJava$$fileSchemaXMsgRspnNoDataFoundJava$$fileSchemaXMsgRspnExceptionJava$$fileSchemaXMsgRspnFileLoadedJava$$fileSchemaXMsgRspnLoggedInJava$$fileSchemaXMsgRspnServerProcExecutedJava$$fileSchemaXMsgRspnLoggedOutJava$$iterate SchemaTables( each tableFiles )$</GenRule>

	<GenRule GenDef="SchemaDef" Name="validatePageData"
		>$iterate SchemaTables( each validatePageData )$</GenRule>
	<GenRule GenDef="Table" Name="validatePageData"
		>$switch PageData yes checkPageDataNoSubclasses default empty$</GenRule>
	<GenRule GenDef="Table" Name="checkPageDataNoSubclasses"
		>$switch HasSubClasses no checkPageDataNormalizedKey default FailAssertPageDataNotAllowedForTablesWithSubclasses$</GenRule>
	<GenRule GenDef="Table" Name="checkPageDataNormalizedKey"
		>$reference BaseClass reference PrimaryKeyIndex checkPageDataNormalizedKey$</GenRule>
	<GenRule GenDef="Index" Name="checkPageDataNormalizedKey"
		>$iterate Columns( each checkPageDataNormalizedKey )$</GenRule>
	<GenRule GenDef="IndexCol" Name="checkPageDataNormalizedKey"
		>$switch IsNullable no empty default FailAssertPageDataNotAllowedForPrimaryKeyWithOptionalColumns$</GenRule>

	<GenRule GenDef="SchemaRef" Name="schemaImportedXsdFiles"
		>$reference RefSchema fileSchemaRqstXsd$$reference RefSchema fileSchemaRspnXsd$</GenRule>

	<GenRule ScopeDef="SchemaDef" GenDef="Table" Name="tableFiles"
		>$fileSchemaXMsgTableMessageFormatterJava$$fileSchemaXMsgClntTableJava$$fileSchemaXMsgRqstTableCreateJava$$fileSchemaXMsgRqstTableReadJava$$fileSchemaXMsgRqstTableReadAllJava$$switch PageData yes fileSchemaXMsgRqstTablePageAllJava default empty$$tableFilesRdByIndexs$$switch PageData yes tableFilesPageByIndexs default empty$$fileSchemaXMsgRqstTableLockJava$$fileSchemaXMsgRqstTableUpdateJava$$fileSchemaXMsgRqstTableDeleteJava$$switch HasChain yes fileSchemaXMsgRqstTableMoveUpJava default empty$$switch HasChain yes fileSchemaXMsgRqstTableMoveDownJava default empty$$tableFilesDelByIndexs$$fileSchemaXMsgRspnTableRecJava$$fileSchemaXMsgRspnTableReadSingleJava$$fileSchemaXMsgRspnTableReadListJava$$fileSchemaXMsgRspnTableCreatedJava$$fileSchemaXMsgRspnTableLockedJava$$fileSchemaXMsgRspnTableUpdatedJava$$fileSchemaXMsgRspnTableDeletedJava$$iterate ServerMethods( each fileSchemaXMsgRqstTableServerMethodJava empty empty )$$iterate ServerMethods( each fileSchemaXMsgRspnTableServerMethodExecutedJava empty empty )$</GenRule>

	<GenRule GenDef="Table" Name="tableFilesDelByIndexs"
		>$switch HasSuperClassRelation yes tableFilesDelByInhIndexs default tableFilesDelByBaseIndexs$</GenRule>

	<GenRule GenDef="Table" Name="tableFilesDelByInhIndexs"
		>$reference SuperClassRelation reference ToTable tableFilesDelByIndexs$$iterate Index( each tableFilesDelByNonPrimaryIndex empty empty )$</GenRule>

	<GenRule GenDef="Table" Name="tableFilesDelByBaseIndexs"
		>$iterate Index( each fileSchemaXMsgRqstTableDelByJava empty empty )$</GenRule>

	<GenRule GenDef="Index" Name="tableFilesDelByNonPrimaryIndex"
		>$switch IsPrimaryIndex yes empty default fileSchemaXMsgRqstTableDelByJava$</GenRule>

	<GenRule GenDef="Table" Name="tableFilesRdByIndexs"
		>$switch HasSuperClassRelation yes tableFilesRdByInhIndexs default tableFilesRdByBaseIndexs$</GenRule>

	<GenRule GenDef="Table" Name="tableFilesRdByInhIndexs"
		>$reference SuperClassRelation reference ToTable tableFilesRdByIndexs$$iterate Index( each tableFilesRdByNonPrimaryIndex empty empty )$</GenRule>

	<GenRule GenDef="Table" Name="tableFilesRdByBaseIndexs"
		>$iterate Index( each tableFilesRdByNonPrimaryIndex empty empty )$</GenRule>

	<GenRule GenDef="Index" Name="tableFilesRdByNonPrimaryIndex"
		>$switch IsPrimaryIndex yes empty default fileSchemaXMsgRqstTableRdByJava$</GenRule>

	<GenRule GenDef="Table" Name="tableFilesPageByIndexs"
		>$switch HasSuperClassRelation yes tableFilesPageByInhIndexs default tableFilesPageByBaseIndexs$</GenRule>

	<GenRule GenDef="Table" Name="tableFilesPageByInhIndexs"
		>$reference SuperClassRelation reference ToTable tableFilesPageByIndexs$$iterate Index( each tableFilesPageByNonPrimaryIndex empty empty )$</GenRule>

	<GenRule GenDef="Table" Name="tableFilesPageByBaseIndexs"
		>$iterate Index( each tableFilesPageByNonPrimaryIndex empty empty )$</GenRule>

	<GenRule GenDef="Index" Name="tableFilesPageByNonPrimaryIndex"
		>$switch IsPrimaryIndex yes empty default ifduplicate_fileSchemaXMsgRqstTablePageByJava$</GenRule>

	<GenRule GenDef="Index" Name="ifduplicate_fileSchemaXMsgRqstTablePageByJava"
		>$switch IsUnique yes empty default fileSchemaXMsgRqstTablePageByJava$</GenRule>

	<GenRule GenDef="Relation" Name="ContainerStyle">$popto Table ContainerStyle$</GenRule>
	<GenRule GenDef="Table" Name="ContainerStyle"
		>$switch ContainerStyleInternal
			Single ContainerStyleSingle
			None ContainerStyleNone
			empty ContainerStyleMultiple
			default ContainerStyleMultiple$</GenRule>

	<GenRule GenDef="Table" Name="ContainerStyleInternal"
		>$iterate ContainerRelations( lone ContainerStyleSingle empty ContainerStyleNone each ContainerStyleYada )$</GenRule>

	<GenRule GenDef="Object" Name="ContainerStyleYada">Yada</GenRule>
	<GenRule GenDef="Object" Name="ContainerStyleSingle">Single</GenRule>
	<GenRule GenDef="Object" Name="ContainerStyleNone">None</GenRule>
	<GenRule GenDef="Object" Name="ContainerStyleMultiple">Multiple</GenRule>

</RuleSet>
