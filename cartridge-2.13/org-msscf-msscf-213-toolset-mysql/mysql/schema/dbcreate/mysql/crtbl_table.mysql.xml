<RuleSet
	xmlns="uri://org.msscf/msscf/2.0.13/cfgenkb-2.13-ruleset"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xsi:schemaLocation="uri://org.msscf/msscf/2.0.13/cfgenkb-2.13-ruleset file:/opt/msscf/2.0.13/xsd/cfgenkb-2.13-ruleset.xsd"
	ToolSet="mysql"
	Name="schema/dbcreate/mysql/crtbl_table.pgsql"
	Revision="2.13"
	Descr="null">

	<GenFile ScopeDef="SchemaDef" GenDef="Table" Name="dbadminfile_CreateTable"
			GenerateOnce="false"
			ModuleName=""
		SourceBundle="dbcreate"
		BasePackageName="$lower SchemaName$"
		SubPackageName="mysql"
		ExpansionClassName="crtbl_$lower DbTableName$"
		ExpansionKeyName="crtbl_$lower DbTableName$"
		ExpansionFileName="$dbadminfilename_CreateTable$">--
--	@(#) $GenPackageDir$/$GenFileName$
$MssDoubleDashScriptLicense$
CREATE TABLE $lower DbSchemaName$.$DbTableName$ ($switch HasSuperClassRelation no dbadminfile_TableClassCodeOptAuditColComma default empty$$iterate Columns( each clause_CreateColComma last clause_CreateCol )$$switch HasSuperClassRelation yes empty default clause_CreateVersionCol$)
<!--
-->		ENGINE=InnoDB$RunSqlStmt$$switch HasPrimaryKey yes clause_PrimaryKey default empty$</GenFile>

	<GenRule GenDef="Table" Name="dbadminfile_TableClassCodeCol">$switch HasSubClasses yes dbadminfile_implTableClassCodeCol default empty$</GenRule>

	<GenRule GenDef="Table" Name="dbadminfile_implTableClassCodeCol">
	classcode	VARCHAR(4) NOT NULL,</GenRule>

	<GenRule GenDef="Table" Name="BaseHasSubClasses"
		>$reference BaseClass HasSubClasses$</GenRule>

	<GenRule GenDef="Table" Name="dbadminfile_TableClassCodeOptAuditColComma">$switch BaseHasSubClasses yes dbadminfile_implTableClassCodeColOptAuditComma default dbadminfile_implTableOptAuditNoClassCodeCol$</GenRule>

	<GenRule GenDef="Table" Name="dbadminfile_TableNoClassCodeOptAuditColComma">$dbadminfile_implTableOptAuditNoClassCodeCol$</GenRule>

	<GenRule GenDef="Table" Name="dbadminfile_implTableOptAuditNoClassCodeCol"
		>$switch HasAuditColumns yes dbadminfile_implTableAuditNoClassCodeCol default empty$</GenRule>

	<GenRule GenDef="Table" Name="dbadminfile_implTableClassCodeColOptAuditComma">$switch HasAuditColumns yes dbadminfile_implTableClassCodeWithAuditColComma default dbadminfile_implTableClassCodeNoAuditColComma$</GenRule>

	<GenRule GenDef="Table" Name="dbadminfile_implTableClassCodeWithAuditColComma"
		>$dbadminfile_implTableClassCodeWithAuditCol$,</GenRule>

	<GenRule GenDef="Table" Name="dbadminfile_implTableClassCodeNoAuditColComma"
		>$dbadminfile_implTableClassCodeCol$</GenRule>

	<GenRule GenDef="Table" Name="dbadminfile_implTableClassCodeWithAuditCol">
	classcode	VARCHAR(4) NOT NULL,
	createdat	DATETIME NOT NULL,
	createdby	VARCHAR(36) NOT NULL,
	updatedat	DATETIME NOT NULL,
	updatedby	VARCHAR(36) NOT NULL</GenRule>

	<GenRule GenDef="Table" Name="dbadminfile_implTableAuditNoClassCodeCol">
	createdat	DATETIME NOT NULL,
	createdby	VARCHAR(36) NOT NULL,
	updatedat	DATETIME NOT NULL,
	updatedby	VARCHAR(36) NOT NULL,</GenRule>

	<GenRule GenDef="Table" Name="dbadminfile_implTableHClassCodeCol">
	classcode	VARCHAR(4) NOT NULL</GenRule>

	<GenRule GenDef="Table" Name="dbadminfile_implTableHClassCodeColComma"
		>$dbadminfile_implTableHClassCodeCol$,</GenRule>

	<GenRule GenDef="Table" Name="dbadminfile_WalkClassCodes">, '$ClassCode$'$iterate TableSubClasses( each dbadminfile_WalkClassCodes empty empty )$</GenRule>

	<GenRule GenDef="Table" Name="clause_PrimaryKey">$reference PrimaryKeyIndex clause_CreatePrimaryIndex$$reference PrimaryKeyIndex clause_PrimaryKey$</GenRule>

	<GenRule ScopeDef="SchemaDef" GenDef="Index" Name="clause_CreatePrimaryIndex">
CREATE$switch IsUnique yes clause_Unique default empty$ INDEX $DbIndexName$ ON $lower DbSchemaName$.$DbTableName$ ($iterate Columns( each clause_PrimaryIndexColComma last clause_PrimaryIndexCol )$ )$RunSqlStmt$</GenRule>

	<GenRule ScopeDef="Table" GenDef="Index" Name="clause_CreatePrimaryIndex">
CREATE$switch IsUnique yes clause_Unique default empty$ INDEX $DbIndexName$ ON $lower DbSchemaName$.$DbTableName$ ($iterate Columns( each clause_PrimaryIndexColComma last clause_PrimaryIndexCol )$ )$RunSqlStmt$</GenRule>

	<GenRule ScopeDef="Index" GenDef="IndexCol" Name="clause_PrimaryIndexColComma">$clause_PrimaryIndexCol$,</GenRule>

	<GenRule ScopeDef="Index" GenDef="IndexCol" Name="clause_PrimaryIndexCol">
	$lower reference Column DbColumnName$</GenRule>

	<GenRule GenDef="Index" Name="clause_PrimaryKey">
ALTER TABLE $lower DbSchemaName$.$DbTableName$
	ADD CONSTRAINT pk_$lower DbIndexName$ PRIMARY KEY ($iterate Columns( each clause_PKeyColComma last clause_PKeyCol )$ )$RunSqlStmt$</GenRule>

	<GenRule ScopeDef="Index" GenDef="IndexCol" Name="clause_PKeyColComma">$clause_PKeyCol$,</GenRule>

	<GenRule ScopeDef="Index" GenDef="IndexCol" Name="clause_PKeyCol"> $lower reference Column DbColumnName$</GenRule>

	<GenRule ScopeDef="Table" GenDef="Atom" Name="clause_CreateColComma">$clause_CreateCol$,</GenRule>

	<GenRule ScopeDef="Table" GenDef="TableCol" Name="clause_CreateColComma">$clause_CreateCol$,</GenRule>

	<GenRule ScopeDef="SchemaDef" GenDef="Table" Name="clause_CreateVersionCol">,
	revision INTEGER NOT NULL</GenRule>

	<GenRule ScopeDef="Table" GenDef="Atom" Name="clause_CreateCol">
	$lower DbColumnName$	$clause_ColType$$switch IsNullable yes clause_Null default clause_NotNull$$optclause_RangeConstraint$</GenRule>

	<GenRule ScopeDef="Table" GenDef="TableCol" Name="clause_CreateCol">
	$lower DbColumnName$	$reference DataType clause_ColType$$switch IsNullable yes clause_Null default clause_NotNull$$optclause_RangeConstraint$</GenRule>

	<GenRule GenDef="Object" Name="clause_Null"> NULL</GenRule>

	<GenRule GenDef="Object" Name="clause_NotNull"> NOT NULL</GenRule>

	<GenRule ScopeDef="Table" GenDef="TableCol" Name="optclause_RangeConstraint">$switch BaseModelAtomClass
	BlobDef empty
	BoolDef bool_RangeConstraint
	EnumDef int16_RangeConstraint
	Int16Def int16_RangeConstraint
	Int32Def int32_RangeConstraint
	Int64Def int64_RangeConstraint
	UInt16Def uint16_RangeConstraint
	UInt32Def uint32_RangeConstraint
	UInt64Def uint64_RangeConstraint
	Id16Gen int16_RangeConstraint
	Id32Gen int32_RangeConstraint
	Id64Gen int64_RangeConstraint
	FloatDef float_RangeConstraint
	DoubleDef double_RangeConstraint
	NumberDef empty
	StringDef empty
	TokenDef empty
	NmTokenDef empty
	NmTokensDef empty
	TextDef empty
	DateDef empty
	TimeDef empty
	TimestampDef empty
	TZDateDef empty
	TZTimeDef empty
	TZTimestampDef empty
	UuidDef empty
	UuidGen empty
	default error$</GenRule>

	<GenRule GenDef="TableCol" Name="DbCheckConstraintName">chk_$lower popto Table DbTableName$$lower DbColumnName$</GenRule>

	<GenRule GenDef="TableCol" Name="clause_ColType">$reference DataType clause_ColType$</GenRule>

	<GenRule GenDef="BlobDef" Name="clause_ColType">BLOB</GenRule>

	<GenRule GenDef="BlobDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="BoolDef" Name="clause_ColType">CHAR(1)</GenRule>

	<GenRule GenDef="BoolDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="Object" Name="bool_RangeConstraint"></GenRule>

	<GenRule GenDef="EnumDef" Name="clause_ColType">SMALLINT</GenRule>

	<GenRule GenDef="EnumDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="Int16Def" Name="clause_ColType">SMALLINT</GenRule>

	<GenRule GenDef="Int16Def" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="Object" Name="int16_RangeConstraint"></GenRule>

	<GenRule GenDef="Int32Def" Name="clause_ColType">INTEGER</GenRule>

	<GenRule GenDef="Int32Def" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="Object" Name="int32_RangeConstraint"></GenRule>

	<GenRule GenDef="Int64Def" Name="clause_ColType">BIGINT</GenRule>

	<GenRule GenDef="Int64Def" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="Object" Name="int64_RangeConstraint"></GenRule>

	<GenRule GenDef="UInt16Def" Name="clause_ColType">INTEGER</GenRule>

	<GenRule GenDef="UInt16Def" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="Object" Name="uint16_RangeConstraint"></GenRule>

	<GenRule GenDef="UInt32Def" Name="clause_ColType">BIGINT</GenRule>

	<GenRule GenDef="UInt32Def" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="Object" Name="uint32_RangeConstraint"></GenRule>

	<GenRule GenDef="UInt64Def" Name="clause_ColType">DECIMAL(20,0)</GenRule>

	<GenRule GenDef="UInt64Def" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="Object" Name="uint64_RangeConstraint"></GenRule>

	<GenRule GenDef="FloatDef" Name="clause_ColType">REAL</GenRule>

	<GenRule GenDef="FloatDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="Object" Name="float_RangeConstraint"></GenRule>

	<GenRule GenDef="DoubleDef" Name="clause_ColType">DOUBLE</GenRule>

	<GenRule GenDef="DoubleDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="NumberDef" Name="clause_ColType">DECIMAL($Digits$,$Precis$)</GenRule>

	<GenRule GenDef="NumberDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="Object" Name="double_RangeConstraint"></GenRule>

	<GenRule GenDef="StringDef" Name="clause_ColType">VARCHAR($MaxLen$)</GenRule>

	<GenRule GenDef="StringDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="TokenDef" Name="clause_ColType">VARCHAR($MaxLen$)</GenRule>

	<GenRule GenDef="TokenDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="NmTokenDef" Name="clause_ColType">VARCHAR($MaxLen$)</GenRule>

	<GenRule GenDef="NmTokenDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="NmTokensDef" Name="clause_ColType">VARCHAR($MaxLen$)</GenRule>

	<GenRule GenDef="NmTokensDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="TextDef" Name="clause_ColType">TEXT</GenRule>

	<GenRule GenDef="TextDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="DateDef" Name="clause_ColType">DATE</GenRule>

	<GenRule GenDef="DateDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="Object" Name="date_RangeConstraint"></GenRule>

	<GenRule GenDef="TimeDef" Name="clause_ColType">TIME</GenRule>

	<GenRule GenDef="TimeDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="TimestampDef" Name="clause_ColType">DATETIME</GenRule>

	<GenRule GenDef="TimestampDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="TZDateDef" Name="clause_ColType">DATETIME</GenRule>

	<GenRule GenDef="TZDateDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="Object" Name="tzdate_RangeConstraint"></GenRule>

	<GenRule GenDef="TZTimeDef" Name="clause_ColType">DATETIME</GenRule>

	<GenRule GenDef="TZTimeDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="TZTimestampDef" Name="clause_ColType">DATETIME</GenRule>

	<GenRule GenDef="TZTimestampDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="UuidDef" Name="clause_ColType">VARCHAR(36)</GenRule>

	<GenRule GenDef="UuidDef" Name="optclause_RangeConstraint"></GenRule>

	<GenRule GenDef="UuidGen" Name="clause_ColType">VARCHAR(36)</GenRule>

	<GenRule GenDef="UuidGen" Name="optclause_RangeConstraint"></GenRule>

</RuleSet>
